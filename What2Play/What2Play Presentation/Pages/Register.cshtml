@page
@model RegisterModel

<h2>Register</h2>

<form method="post">
    <div>
        <label>Email</label>
        <input asp-for="Email" />
    </div>

    <div>
        <label>Password</label>
        <input asp-for="Password" type="password" />
    </div>

    <button type="submit">Create account</button>

    <div asp-validation-summary="All"></div>
</form>
